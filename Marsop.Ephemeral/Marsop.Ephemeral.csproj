<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Ephemeral</PackageId>
    <PackageTags>Time;Timestamps;Time Intervals</PackageTags>
    <Version>0.1.7</Version>
    <Authors>Alberto Gregorio</Authors>
    <Owners>albertogregorio</Owners>
    <Company>albertogregorio.com</Company>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>.NET Library to handle time intervals (composite start and end)</Description>
    <ProjectUrl>https://albertogregorio.com/ephemeral</ProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/marsop/ephemeral/master/docs/img/Ephemeral64x64.png</PackageIconUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>0.1.7.0</AssemblyVersion>
    <FileVersion>0.1.7.0</FileVersion>
    <InformationalVersion>0.1.7+2.Branch.master.Sha.54eb3aaa02fc230e989b6bdc0691cec2e34f5e06</InformationalVersion>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Marsop.Ephemeral.Tests\**" />
    <EmbeddedResource Remove="Marsop.Ephemeral.Tests\**" />
    <None Remove="Marsop.Ephemeral.Tests\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Optional" Version="4.0.0" />
  </ItemGroup>
</Project>